<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="MyAccount Add Address"
    sling:resourceType="cq/gui/components/authoring/dialog"
    helpPath="https://todo.frontier.com/a/future/help/path/for/authors.html">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/foundation/container">
        <layout
            jcr:primaryType="nt:unstructured"
            sling:resourceType="granite/ui/components/foundation/layouts/tabs"
            type="nav"/>
        <items jcr:primaryType="nt:unstructured">
            <column jcr:primaryType="nt:unstructured"
				jcr:title="Add New Address Labels"
				sling:resourceType="granite/ui/components/foundation/container">
				<layout jcr:primaryType="nt:unstructured"
					sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns" />
				<items jcr:primaryType="nt:unstructured">
					<columns jcr:primaryType="nt:unstructured"
						sling:resourceType="granite/ui/components/foundation/container">
						<items jcr:primaryType="nt:unstructured">
							<addNewAddressHeader
								jcr:primaryType="nt:unstructured"
								sling:resourceType="granite/ui/components/foundation/form/textfield"
								fieldDescription="if left empty defaults to 'Create a New Address'"
								fieldLabel="Add New Address Header" name="./addNewAddressHeader" />
							<companyLabel jcr:primaryType="nt:unstructured"
								sling:resourceType="granite/ui/components/foundation/form/textfield"
								fieldDescription="if left empty defaults to 'Company'"
								fieldLabel="Company Label" name="./companyLabel" />
							<fullNameLabel jcr:primaryType="nt:unstructured"
								sling:resourceType="granite/ui/components/foundation/form/textfield"
								fieldDescription="if left empty defaults to 'Full Name'"
								fieldLabel="Full Name Label" name="./fullNameLabel" />
							<addressLine1Label
								jcr:primaryType="nt:unstructured"
								sling:resourceType="granite/ui/components/foundation/form/textfield"
								fieldDescription="if left empty defaults to 'Address Line 1'"
								fieldLabel="Address Line 1 Label" name="./addressLine1Label" />
							<addressLine1Hint jcr:primaryType="nt:unstructured"
								sling:resourceType="granite/ui/components/foundation/form/textfield"
								fieldDescription="if left empty defaults to '123 Example Street'"
								fieldLabel="Address Line 1 Hint" name="./addressLine1Hint" />
							<addressLine2Label
								jcr:primaryType="nt:unstructured"
								sling:resourceType="granite/ui/components/foundation/form/textfield"
								fieldDescription="if left empty defaults to 'Address Line 2'"
								fieldLabel="Address Line 2 Label" name="./addressLine2Label" />
							<addressLine2Hint jcr:primaryType="nt:unstructured"
								sling:resourceType="granite/ui/components/foundation/form/textfield"
								fieldDescription="if left empty defaults to 'Suite 55C'"
								fieldLabel="Address Line 2 Hint" name="./addressLine2Hint" />
							<cityLabel jcr:primaryType="nt:unstructured"
								sling:resourceType="granite/ui/components/foundation/form/textfield"
								fieldDescription="if left empty defaults to 'City'"
								fieldLabel="City Label" name="./cityLabel" />
							<stateLabel jcr:primaryType="nt:unstructured"
								sling:resourceType="granite/ui/components/foundation/form/textfield"
								fieldDescription="if left empty defaults to 'State'"
								fieldLabel="State Label" name="./stateLabel" />
							<zipLabel jcr:primaryType="nt:unstructured"
								sling:resourceType="granite/ui/components/foundation/form/textfield"
								fieldDescription="if left empty defaults to 'Zip'"
								fieldLabel="Zip Label" name="./zipLabel" />
							<countryLabel jcr:primaryType="nt:unstructured"
								sling:resourceType="granite/ui/components/foundation/form/textfield"
								fieldDescription="if left empty defaults to 'Country'"
								fieldLabel="Country Label" name="./countryLabel" />
							<buildingLabel jcr:primaryType="nt:unstructured"
								sling:resourceType="granite/ui/components/foundation/form/textfield"
								fieldDescription="if left empty defaults to 'Building'"
								fieldLabel="Building Label" name="./buildingLabel" />
							<phoneLabel jcr:primaryType="nt:unstructured"
								sling:resourceType="granite/ui/components/foundation/form/textfield"
								fieldDescription="if left empty defaults to 'Phone'"
								fieldLabel="Phone Label" name="./phoneLabel" />
							<faxLabel jcr:primaryType="nt:unstructured"
								sling:resourceType="granite/ui/components/foundation/form/textfield"
								fieldDescription="if left empty defaults to 'Fax'"
								fieldLabel="Fax Label" name="./faxLabel" />
							<saveButtonText jcr:primaryType="nt:unstructured"
								sling:resourceType="granite/ui/components/foundation/form/textfield"
								fieldDescription="if left empty defaults to 'Save'"
								fieldLabel="Save Button Text" name="./saveButtonText" />
						</items>
					</columns>
				</items>
			</column>
        </items>
    </content>
</jcr:root>
